{
  "name": "figma-to-code-export-plugin",
  "version": "1.0.0",
  "description": "Figma plugin for exporting nodes to code",
  "main": "dist/plugin.js",
  "type": "module",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node server.js",
    "build": "webpack --config webpack.config.js --mode production && webpack --config webpack.ui.config.js --mode production",
    "watch": "webpack --config webpack.config.js --mode development --watch && webpack --config webpack.ui.config.js --mode development --watch",
    "dev": "concurrently -n \"webpack,server\" -c \"yellow,cyan\" \"npm run watch\" \"node server.js\"",
    "build:ui": "webpack --config webpack.ui.config.js --mode production",
    "build:backend": "webpack --config webpack.config.js --mode production"
  },
  "keywords": [],
  "author": "Ian Proefriedt",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "fontkit": "^2.0.4",
    "node-fetch": "^3.3.2",
    "npm-which": "^3.0.1",
    "ttf2woff2": "^6.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "babel-loader": "^9.1.3",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.10.0",
    "html-inline-script-webpack-plugin": "^3.2.1",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "mini-css-extract-plugin": "^2.9.2",
    "style-loader": "^3.3.4",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
